.search-wrapper {
  @apply text-zinc-800;
}

.search-input {
  @apply rounded-md leading-10 px-3;
}

.search-output {
  @apply absolute shadow-3xl border-transparent z-10 mx-0 px-4 pt-3 -mt-1 bg-white;
  min-height: 25rem;
  min-width: 32rem;

  & .search-header {
    @apply flex justify-between;
  }

  & .search-results {
    @apply flex justify-between;
  }
}
